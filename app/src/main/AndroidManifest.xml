<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.multi">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QMultiImagesSelectView">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.QMultiImagesSelectView.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lzy.imagepicker.ui.ImageBaseActivity" />
        <activity android:name="com.lzy.imagepicker.ui.ImageCropActivity" />
        <activity android:name="com.lzy.imagepicker.ui.ImageGridActivity" />
        <activity android:name="com.lzy.imagepicker.ui.ImagePreviewActivity" />
        <activity android:name="com.lzy.imagepicker.ui.ImagePreviewBaseActivity" />
        <activity android:name="com.lzy.imagepicker.ui.ImagePreviewDelActivity" />
        <activity android:name="com.lzy.imagepicker.ui.ImagePurePreviewActivity" />
        <activity android:name="com.lzy.imagepicker.ui.ImageSingleSelectActivity" />
    </application>
</manifest>