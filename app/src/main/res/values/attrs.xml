<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="required" format="boolean" />

    <declare-styleable name="MultiImagesSelectView">
        <attr name="required" />
        <attr name="left_title_text" format="string" />
        <attr name="left_icon_visibility" format="string" />
        <attr name="left_icon" format="reference" />
        <attr name="right_title_visibility" format="string" />
        <attr name="right_title_text" format="string" />
        <attr name="sub_title_visibility" format="string" />
        <attr name="sub_title_text" format="string" />
        <attr name="display_mode" format="boolean" />
        <attr name="span_count" format="integer" />
        <attr name="max_select_count" format="integer" />
    </declare-styleable>

    <declare-styleable name="CropImageView">
        <attr name="cropMaskColor" format="color" />
        <attr name="cropBorderColor" format="color" />
        <attr name="cropBorderWidth" format="dimension" />
        <attr name="cropFocusWidth" format="dimension" />
        <attr name="cropFocusHeight" format="dimension" />
        <attr name="cropStyle" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
        </attr>
    </declare-styleable>
</resources>